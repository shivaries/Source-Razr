<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.ListView.NoDivider" parent="@android:style/Widget.ListView">
        <item name="android:fadingEdge">none</item>
        <item name="android:listSelector">@null</item>
        <item name="android:cacheColorHint">@null</item>
        <item name="android:divider">@null</item>
        <item name="android:dividerHeight">0.0dip</item>
    </style>
    <style name="Widget.TabWidget.Secondary" parent="@android:style/Widget.TabWidget">
        <item name="android:textColor">@color/secondary_tab_text</item>
        <item name="android:shadowColor">#e6ffffff</item>
    </style>
    <style name="Widget.Button" parent="@android:style/Widget">
        <item name="android:textAppearance">?android:textAppearanceSmallInverse</item>
        <item name="android:textColor">@color/button_text</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@android:drawable/btn_default</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:shadowColor">@color/button_text_shadow</item>
        <item name="android:shadowDy">@integer/button_text_shadow_dy</item>
        <item name="android:shadowRadius">0.2</item>
    </style>
    <style name="Widget.Button.Bar" parent="@style/Widget.Button">
        <item name="android:textSize">@dimen/button_bar_text_size</item>
        <item name="android:background">@drawable/btn_bar</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="BottomButtonBar">
        <item name="android:textSize">@dimen/button_bar_text_size</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:background">@drawable/bottom_button_bar</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="TextAppearanceSecondaryTab">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">@color/secondary_tab_text</item>
    </style>
    <style name="TextAppearanceSecondaryTab.Left" parent="@style/TextAppearanceSecondaryTab">
        <item name="android:background">@drawable/secondary_tab_left</item>
    </style>
    <style name="TextAppearanceSecondaryTab.Middle" parent="@style/TextAppearanceSecondaryTab">
        <item name="android:background">@drawable/secondary_tab_middle</item>
    </style>
    <style name="TextAppearanceSecondaryTab.Right" parent="@style/TextAppearanceSecondaryTab">
        <item name="android:background">@drawable/secondary_tab_right</item>
    </style>
    <style name="TextViewFloatPanelButton">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textColor">@color/textview_floatpanel_button_text_color</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/textview_floatpanel_button_bg</item>
        <item name="android:layout_width">60.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">38.0dip</item>
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">#ccffffff</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">0.2</item>
    </style>
    <style name="ResourceBrowser">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ResourceBrowser.ListItem" parent="@style/ResourceBrowser">
        <item name="android:layout_width">1.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="ResourceBrowser.HorizontalLine" parent="@style/ResourceBrowser">
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="ResourceBrowser.ResourceImage" parent="@style/ResourceBrowser">
        <item name="android:background">@drawable/resource_thumbnail_bg</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">centerCrop</item>
        <item name="android:adjustViewBounds">true</item>
    </style>
    <style name="ResourceBrowser.ResourceDescription" parent="@style/ResourceBrowser">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">left|center</item>
        <item name="android:paddingLeft">6.0dip</item>
        <item name="android:paddingRight">6.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ResourceBrowser.ResourceDescription.Vertical" parent="@style/ResourceBrowser.ResourceDescription">
        <item name="android:textColor">?android:textColorPrimaryInverse</item>
        <item name="android:layout_gravity">bottom</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ResourceBrowser.ResourceDescription.Horizontal" parent="@style/ResourceBrowser.ResourceDescription">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:paddingLeft">10.0dip</item>
        <item name="android:layout_width">1.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="ResourceBrowser.ResourceProperty" parent="@style/ResourceBrowser">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">1.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="ResourceBrowser.ResourceProperty.Title" parent="@style/ResourceBrowser.ResourceProperty">
        <item name="android:gravity">right</item>
        <item name="android:layout_weight">2.0</item>
    </style>
    <style name="ResourceBrowser.ResourceProperty.Content" parent="@style/ResourceBrowser.ResourceProperty">
        <item name="android:layout_marginLeft">10.0dip</item>
        <item name="android:layout_weight">3.0</item>
    </style>
    <style name="Theme" parent="@android:style/Theme.Holo" />
    <style name="Theme.Light" parent="@android:style/Theme.Holo.Light">
        <item name="android:expandableListPreferredItemPaddingLeft">35.0dip</item>
        <item name="android:expandableListViewStyle">@style/Widget.Holo.Light.ExpandableListView</item>
        <item name="android:expandableListViewWhiteStyle">@style/Widget.Holo.Light.ExpandableListView.White</item>
        <item name="android:actionBarStyle">@style/Widget.Holo.Light.ActionBar.Solid.HomeUp</item>
        <item name="android:actionBarTabTextStyle">@style/Widget.Holo.Light.ActionBar.TabText</item>
        <item name="android:actionOverflowButtonStyle">@style/Widget.Holo.Light.ActionButton.Overflow</item>
        <item name="android:homeAsUpIndicator">@drawable/ab_home_up</item>
    </style>
    <style name="Widget.Holo.Light.ActionBar.Solid.HomeUp" parent="@android:style/Widget.Holo.Light.ActionBar.Solid">
        <item name="android:logo">@drawable/ab_home_up_divider</item>
        <item name="android:displayOptions">useLogo|showHome|homeAsUp|showTitle</item>
    </style>
    <style name="EditorCustomActionButton" parent="@android:style/Widget.Holo.Light.ActionButton">
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/custom_action_button</item>
        <item name="android:focusable">true</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="EditorCustomActionButtonImage">
        <item name="android:padding">4.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="EditorCustomActionButtonText">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textColor">@color/editor_custom_action_button_text_color</item>
        <item name="android:ellipsize">none</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">4.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="EditorCustomActionBarTheme" parent="@android:style/Theme.Holo">
        <item name="android:background">@drawable/custom_action_bar_bg</item>
    </style>
    <style name="TextAppearance.ListView" parent="@android:attr/textAppearance">
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
    <style name="TextAppearance.ListView.PrimaryItem" parent="@style/TextAppearance.ListView">
        <item name="android:textSize">@dimen/list_primary_text_size</item>
        <item name="android:textColor">@color/primary_text</item>
        <item name="android:shadowColor">@color/primary_text_color_shadow</item>
    </style>
    <style name="TextAppearance.ListView.SecondaryItem" parent="@style/TextAppearance.ListView">
        <item name="android:textSize">@dimen/list_secondary_text_size</item>
        <item name="android:textColor">@color/secondary_text</item>
        <item name="android:shadowColor">@color/secondary_text_color_shadow</item>
    </style>
    <style name="TextAppearance.StatusBar" parent="@android:style/TextAppearance">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
    </style>
    <style name="TextAppearance.StatusBar.Ticker" parent="@style/TextAppearance.StatusBar" />
    <style name="Theme.Holo.Light.Dialog.Alert" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowMinWidthMajor">@android:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@android:dimen/dialog_min_width_minor</item>
    </style>
    <style name="Animation.ZoomButtons" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>
    <style name="Widget.Holo.Light.ActionButton.Overflow" parent="@android:style/Widget.Holo.Light.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_menu_moreoverflow_holo_light</item>
    </style>
    <style name="Theme.Dialog.Alert" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:itemTextAppearance">@android:style/TextAppearance.Large.Inverse</item>
        <item name="android:textAppearanceListItem">@android:style/TextAppearance.Large.Inverse</item>
        <item name="android:textAppearanceListItemSmall">@android:style/TextAppearance.Large.Inverse</item>
    </style>
    <style name="Widget.Button.Transparent" parent="@android:style/Widget.Button">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/android_btn_default_transparent</item>
    </style>
    <style name="Theme.Panel.Volume" parent="@android:style/Theme.Panel">
        <item name="android:windowAnimationStyle">@style/Animation.VolumePanel</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>
    <style name="Animation.VolumePanel" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@null</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>
    <style name="SlidingButtonStyle">
        <item name="buttonFrame">@drawable/sliding_btn_frame</item>
        <item name="buttonSlider">@drawable/sliding_btn_slider</item>
        <item name="buttonSliderPressed">@drawable/sliding_btn_slider_pressed</item>
        <item name="buttonOnDisable">@drawable/sliding_btn_on_disable</item>
        <item name="buttonOffDisable">@drawable/sliding_btn_off_disable</item>
        <item name="buttonMask">@drawable/sliding_btn_mask</item>
        <item name="buttonBar">@drawable/sliding_btn_bar</item>
    </style>
    <style name="Widget.Holo.Light.ExpandableListView" parent="@android:style/Widget.Holo.Light.ExpandableListView">
        <item name="android:groupIndicator">@drawable/expander_group_holo_light</item>
        <item name="android:indicatorLeft">16.0dip</item>
    </style>
    <style name="Widget.Holo.Light.ExpandableListView.White" parent="@style/Widget.Holo.Light.ExpandableListView" />
    <style name="Widget.Holo.Light.ActionBar.TabText" parent="@android:style/Widget.Holo.Light.ActionBar.TabText.Inverse">
        <item name="android:textColor">@color/primary_tab_text</item>
    </style>
    <style name="AlphabetFastIndexerStyle">
        <item name="indexerTable">@array/alphabet_table</item>
        <item name="indexerTextSize">@dimen/alphabet_indexer_text_size</item>
        <item name="indexerTextColor">@color/alphabet_indexer_text_color</item>
        <item name="indexerTextHighlightColor">@color/alphabet_indexer_highlight_text_color</item>
        <item name="indexerBackground">@drawable/alphabet_indexer_bg</item>
        <item name="overlayBackground">@drawable/alphabet_indexer_overlay</item>
        <item name="overlayMarginLeft">@dimen/alphabet_indexer_overlay_text_size</item>
        <item name="overlayMarginTop">@dimen/alphabet_indexer_overlay_padding_top</item>
        <item name="overlayTextSize">@dimen/alphabet_indexer_overlay_text_size</item>
        <item name="overlayTextColor">@color/alphabet_indexer_overlay_text_color</item>
    </style>
    <style name="resource_circle_progressbar">
        <item name="android:scaleType">fitCenter</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@drawable/resource_circle_progressbar_indeterminate</item>
        <item name="android:progressDrawable">@drawable/resource_circle_progressbar_determinate</item>
        <item name="android:indeterminateDuration">3500</item>
        <item name="android:indeterminateBehavior">repeat</item>
    </style>
    <style name="ResourceBrowser.ResourceDescription.Font" parent="@style/ResourceBrowser.ResourceDescription">
        <item name="android:textAppearance">?android:textAppearanceSmall</item>
        <item name="android:textColor">@android:color/darker_gray</item>
        <item name="android:gravity">bottom|center</item>
        <item name="android:paddingBottom">30.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
</resources>
